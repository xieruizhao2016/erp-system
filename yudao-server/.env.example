# ============================================
# 后端服务环境变量配置文件示例
# ============================================
# 复制此文件为 .env 并填入实际配置值

# ============================================
# 数据库配置
# ============================================
# 主数据库
DATASOURCE_MASTER_URL=jdbc:mysql://127.0.0.1:3306/ruoyi-vue-pro?useSSL=false&serverTimezone=Asia/Shanghai&allowPublicKeyRetrieval=true&nullCatalogMeansCurrent=true&rewriteBatchedStatements=true
DATASOURCE_MASTER_USERNAME=root
DATASOURCE_MASTER_PASSWORD=your_password_here

# 从数据库（可选）
DATASOURCE_SLAVE_URL=jdbc:mysql://127.0.0.1:3306/ruoyi-vue-pro?useSSL=false&serverTimezone=Asia/Shanghai&allowPublicKeyRetrieval=true&nullCatalogMeansCurrent=true&rewriteBatchedStatements=true
DATASOURCE_SLAVE_USERNAME=root
DATASOURCE_SLAVE_PASSWORD=your_password_here

# ============================================
# Redis 配置
# ============================================
REDIS_HOST=127.0.0.1
REDIS_PORT=6379
REDIS_DATABASE=0
REDIS_PASSWORD=

# ============================================
# MyBatis Plus 加密密钥
# ============================================
MYBATIS_PLUS_ENCRYPTOR_PASSWORD=your_encryptor_password_here

# ============================================
# API 加密配置
# ============================================
API_ENCRYPT_REQUEST_KEY=your_request_key_here
API_ENCRYPT_RESPONSE_KEY=your_response_key_here

# ============================================
# AI 服务配置（根据需要配置）
# ============================================
# 文心一言
AI_QIANFAN_API_KEY=your_api_key_here
AI_QIANFAN_SECRET_KEY=your_secret_key_here

# 智谱 AI
AI_ZHIPUAI_API_KEY=your_api_key_here

# OpenAI
AI_OPENAI_API_KEY=your_api_key_here
AI_OPENAI_BASE_URL=https://api.openai.com

# Azure OpenAI
AI_AZURE_ENDPOINT=your_endpoint_here

# Anthropic Claude
AI_ANTHROPIC_API_KEY=your_api_key_here

# Ollama
AI_OLLAMA_BASE_URL=http://127.0.0.1:11434
AI_OLLAMA_MODEL=llama3

# Stability AI
AI_STABILITYAI_API_KEY=your_api_key_here

# 通义千问
AI_DASHSCOPE_API_KEY=your_api_key_here

# Minimax
AI_MINIMAX_API_KEY=your_api_key_here

# 月之暗面（KIMI）
AI_MOONSHOT_API_KEY=your_api_key_here

# DeepSeek
AI_DEEPSEEK_API_KEY=your_api_key_here
AI_DEEPSEEK_MODEL=deepseek-chat

# 谷歌 Gemini
AI_GEMINI_ENABLE=false
AI_GEMINI_API_KEY=your_api_key_here
AI_GEMINI_MODEL=gemini-2.5-flash

# 字节豆包
AI_DOUBAO_ENABLE=false
AI_DOUBAO_API_KEY=your_api_key_here
AI_DOUBAO_MODEL=doubao-1-5-lite-32k-250115

# 腾讯混元
AI_HUNYUAN_ENABLE=false
AI_HUNYUAN_API_KEY=your_api_key_here
AI_HUNYUAN_MODEL=hunyuan-turbo

# 硅基流动
AI_SILICONFLOW_ENABLE=false
AI_SILICONFLOW_API_KEY=your_api_key_here
AI_SILICONFLOW_MODEL=deepseek-ai/DeepSeek-R1-Distill-Qwen-7B

# 讯飞星火
AI_XINGHUO_ENABLE=false
AI_XINGHUO_APP_KEY=your_app_key_here
AI_XINGHUO_SECRET_KEY=your_secret_key_here
AI_XINGHUO_MODEL=x1

# 百川智能
AI_BAICHUAN_ENABLE=false
AI_BAICHUAN_API_KEY=your_api_key_here
AI_BAICHUAN_MODEL=Baichuan4-Turbo

# Midjourney
AI_MIDJOURNEY_ENABLE=false
AI_MIDJOURNEY_BASE_URL=https://api.holdai.top/mj
AI_MIDJOURNEY_API_KEY=your_api_key_here
AI_MIDJOURNEY_NOTIFY_URL=http://your-domain.com/admin-api/ai/image/midjourney/notify

# Suno
AI_SUNO_ENABLE=false
AI_SUNO_BASE_URL=http://127.0.0.1:3001

# Web Search
AI_WEB_SEARCH_ENABLE=false
AI_WEB_SEARCH_API_KEY=your_api_key_here

# ============================================
# 微信公众号/小程序配置
# ============================================
WX_MP_APP_ID=your_app_id_here
WX_MP_SECRET=your_secret_here

WX_MINIAPP_APPID=your_appid_here
WX_MINIAPP_SECRET=your_secret_here

# ============================================
# 第三方登录配置
# ============================================
# 钉钉
JUSTAUTH_DINGTALK_CLIENT_ID=your_client_id_here
JUSTAUTH_DINGTALK_CLIENT_SECRET=your_client_secret_here

# 企业微信
JUSTAUTH_WECHAT_ENTERPRISE_CLIENT_ID=your_client_id_here
JUSTAUTH_WECHAT_ENTERPRISE_CLIENT_SECRET=your_client_secret_here
JUSTAUTH_WECHAT_ENTERPRISE_AGENT_ID=your_agent_id_here

# ============================================
# 其他服务配置
# ============================================
# QQ 地图密钥
TENCENT_LBS_KEY=your_key_here

# 快递鸟
EXPRESS_KD_NIAO_API_KEY=your_api_key_here
EXPRESS_KD_NIAO_BUSINESS_ID=your_business_id_here

# 快递100
EXPRESS_KD100_KEY=your_key_here
EXPRESS_KD100_CUSTOMER=your_customer_here
